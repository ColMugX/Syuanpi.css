@keyframes fadeInUp
  from
    opacity 0
    transform translate3d(0, 100%, 0)
  30%
    opacity 0
  to
    opacity 1
    transform translate3d(0, 0, 0)

@keyframes fadeInUp
  from
    opacity 0
    transform translate3d(0, 50px, 0)
  30%
    opacity 0
  to
    opacity 1
    transform translate3d(0, 0, 0)

.fadeInUp
  animation-name: fadeInUp
  animation-timing-function: $enterbz

.fadeInUpShort
  animation-name: fadeInUpShort
  animation-timing-function: $enterbz
